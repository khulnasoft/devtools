{
  "name": "{{name}}",
  "version": "{{pluginVersion}}",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "license": "{{license}}",
{{#if privatePackage}}
  "private": {{privatePackage}},
{{/if}}
  "publishConfig": {
{{#if npmRegistry}}
    "registry": "{{npmRegistry}}",
{{/if}}
    "access": "public",
    "main": "dist/index.cjs.js",
    "types": "dist/index.d.ts"
  },
  "devtools": {
    "role": "backend-plugin"
  },
  "scripts": {
    "start": "devtools-cli package start",
    "build": "devtools-cli package build",
    "lint": "devtools-cli package lint",
    "test": "devtools-cli package test",
    "clean": "devtools-cli package clean",
    "prepack": "devtools-cli package prepack",
    "postpack": "devtools-cli package postpack"
  },
  "dependencies": {
    "@devtools/backend-defaults": "{{versionQuery '@devtools/backend-defaults'}}",
    "@devtools/backend-plugin-api": "{{versionQuery '@devtools/backend-plugin-api'}}",
    "@devtools/catalog-client": "{{versionQuery '@devtools/catalog-client'}}",
    "@devtools/errors": "{{versionQuery '@devtools/errors'}}",
    "@devtools/plugin-catalog-node": "{{versionQuery '@devtools/plugin-catalog-node'}}",
    "express": "{{versionQuery 'express' '4.17.1'}}",
    "express-promise-router": "{{versionQuery 'express-promise-router' '4.1.0'}}",
    "zod": "{{versionQuery 'zod' '3.22.4'}}"
  },
  "devDependencies": {
    "@devtools/backend-test-utils": "{{versionQuery '@devtools/backend-test-utils'}}",
    "@devtools/cli": "{{versionQuery '@devtools/cli'}}",
    "@types/express": "{{versionQuery '@types/express' '4.17.6'}}",
    "@types/supertest": "{{versionQuery '@types/supertest' '2.0.12'}}",
    "supertest": "{{versionQuery 'supertest' '6.2.4'}}"
  },
  "files": [
    "dist"
  ]
}
