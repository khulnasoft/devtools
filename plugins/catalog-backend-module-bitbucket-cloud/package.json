{
  "name": "@devtools/plugin-catalog-backend-module-bitbucket-cloud",
  "version": "0.4.4-next.0",
  "description": "A Devtools catalog backend module that helps integrate towards Bitbucket Cloud",
  "devtools": {
    "role": "backend-plugin-module",
    "pluginId": "catalog",
    "pluginPackage": "@devtools/plugin-catalog-backend"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "devtools"
  ],
  "homepage": "https://devtools.khulnasoft.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/khulnasoft/devtools",
    "directory": "plugins/catalog-backend-module-bitbucket-cloud"
  },
  "license": "Apache-2.0",
  "exports": {
    ".": "./src/index.ts",
    "./alpha": "./src/alpha.ts",
    "./package.json": "./package.json"
  },
  "main": "src/index.ts",
  "types": "src/index.ts",
  "typesVersions": {
    "*": {
      "alpha": [
        "src/alpha.ts"
      ],
      "package.json": [
        "package.json"
      ]
    }
  },
  "files": [
    "config.d.ts",
    "dist"
  ],
  "scripts": {
    "build": "devtools-cli package build",
    "clean": "devtools-cli package clean",
    "lint": "devtools-cli package lint",
    "prepack": "devtools-cli package prepack",
    "postpack": "devtools-cli package postpack",
    "start": "devtools-cli package start",
    "test": "devtools-cli package test"
  },
  "dependencies": {
    "@devtools/backend-plugin-api": "workspace:^",
    "@devtools/catalog-client": "workspace:^",
    "@devtools/catalog-model": "workspace:^",
    "@devtools/config": "workspace:^",
    "@devtools/integration": "workspace:^",
    "@devtools/plugin-bitbucket-cloud-common": "workspace:^",
    "@devtools/plugin-catalog-common": "workspace:^",
    "@devtools/plugin-catalog-node": "workspace:^",
    "@devtools/plugin-events-node": "workspace:^",
    "uuid": "^11.0.0"
  },
  "devDependencies": {
    "@devtools/backend-test-utils": "workspace:^",
    "@devtools/cli": "workspace:^",
    "@devtools/plugin-events-backend-test-utils": "workspace:^",
    "luxon": "^3.0.0",
    "msw": "^1.0.0"
  },
  "configSchema": "config.d.ts"
}
